@{
    Layout = "_Layout";
}

<!DOCTYPE html>

<html lang="en">
<head>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.css">
    <meta name="viewport" content="width=device-width" />
    <title>Home</title>
</head>
<body>
    <!-- top section -->
    <div class="top-section">
        <div class="container-fluid container-px">
            <h3 class="fw-semibold mb-0">
                <span class="blue-primary">Fast Track Buyer</span> - Application
            </h3>
        </div>
    </div>
    <div class="addlistingform min-50vh">
        <div class="container-fluid container-px">
            <div class="add-property">
                <div class="row">
                    <!-- fields for wholesaler start -->
                    <div class="col-xl-12 col-md-12 col-12 mb-3">
                        <div class="container-fluid">
                            <div class="row justify-content-center">
                                <div class="col-12 col-md-12 col-lg-12 p-0 mt-3 mb-2">
                                    <div class="card px-0 pt-4 pb-0 mt-3 mb-3">

                                        <form id="msform">
                                            <!-- progressbar -->

                                            <div class="progress">
                                                <div class="progress-bar progress-bar-striped progress-bar-animated"
                                                    role="" aria-valuemin="0" aria-valuemax="100">7%</div>
                                            </div> <br>
                                            <!-- fieldsets -->
                                            <section class="fieldset" id="tab1">
                                                <div class="form-card">
                                                    <div class="row">
                                                        <div class="col-12">
                                                            <h2 class="fs-title">
                                                                Please Confirm The Information
                                                                <!-- Please confirm the information on file -->
                                                            </h2>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-3">
                                                        <!-- <h5>Personal Information</h5> -->
                                                        <div class="col-xl-3 col-md-6 mb-3">
                                                            <label for="fName" class="form-label">First Name</label>
                                                            <input type="text" class="input-field" id="fName"
                                                                placeholder="First Name" value="Anna" disabled>
                                                        </div>
                                                        <div class="col-xl-3 col-md-6 mb-3">
                                                            <label for="lName" class="form-label">Last Name</label>
                                                            <input type="text" class="input-field" id="lName"
                                                                placeholder="Last Name" value="Swift" disabled>
                                                        </div>
                                                        <div class="col-xl-3 col-md-6 mb-3">
                                                            <label for="mobilePhoneId" class="form-label">Phone</label>
                                                            <input type="tel" class="input-field" id="mobilePhoneId"
                                                                value="+1-(713) 480-0002" disabled>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-2 mb-3">
                                                        <div class="col-xl-6">
                                                            <div class="row">
                                                                <div class="col-xl-6 col-md-6 mb-2">
                                                                    <label for="address1" class="form-label">
                                                                        Address line 1</label>
                                                                    <input type="text" class="input-field" id="address1"
                                                                        placeholder="Address l">
                                                                </div>
                                                                <div class="col-xl-6 col-md-6 mb-2">
                                                                    <label for="address2" class="form-label">
                                                                        Address line 2</label>
                                                                    <input type="text" class="input-field" id="address2"
                                                                        placeholder="Address 2">
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-xl-6">
                                                            <div class="row">
                                                                <div class="col-xl-4 col-md-6 mb-2">
                                                                    <label for="CityField"
                                                                        class="form-label">City</label>
                                                                    <input type="text" class="input-field"
                                                                        id="CityField" placeholder="City">
                                                                </div>
                                                                <div class="col-xl-4 col-md-6 mb-2">
                                                                    <label for="stateField"
                                                                        class="form-label">State</label>
                                                                    <input type="text" class="input-field"
                                                                        id="stateField" placeholder="State">
                                                                </div>
                                                                <div class="col-xl-4 col-md-6 mb-2">
                                                                    <label for="zipCode" class="form-label">Zip
                                                                        code</label>
                                                                    <input type="text" class="input-field" id="zipCode"
                                                                        placeholder="Zip code">
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <button type="button" name=""
                                                    class="next btn-sign btn-pbr float-end">Next</button>
                                                <button type="button" name="next"
                                                    class="close btn-sign btn-pbr float-end">Save and Close
                                                </button>
                                            </section>
                                            <section class="fieldset test" id="tab2">
                                                <div class="form-card">
                                                    <div class="row">
                                                        <div class="col-12">
                                                            <h2 class="fs-title text-capitalize">Work experience</h2>
                                                        </div>
                                                        <div class="col-12">
                                                            <h6>Please enter at least 5 years of work history</h6>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-2">
                                                        <div class="col-xl-3 col-md-6 mb-3">
                                                            <label for="CompanyNameFst" class="form-label text-start">
                                                                Company name</label>
                                                            <input type="text" class="input-field" id="CompanyNameFst"
                                                                placeholder="Company name">
                                                        </div>
                                                        <div class="col-xl-4 col-md-6 mb-2">
                                                            <div class="mb-3">
                                                                <label class="form-label">Work
                                                                    date</label>
                                                                <div class="d-flex align-items-center">
                                                                    <input type="date" class="input-field">
                                                                    <span class="mx-2">to</span>
                                                                    <input type="date" class="input-field">
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-xl-4 col-md-6 mb-3">
                                                            <label for="WorkDesc" class="form-label text-start">
                                                                Description</label>
                                                            <input type="text" class="input-field" id="WorkDesc"
                                                                placeholder="Description">
                                                        </div>
                                                        <div class="col-xl-1 col-md-1 mt-6 sm-mt-0">
                                                            <span class="ms-2">
                                                                <svg xmlns="http://www.w3.org/2000/svg" width="20"
                                                                    height="20" viewBox="0 0 20 20" fill="none">
                                                                    <path
                                                                        d="M10 0C4.477 0 0 4.477 0 10C0 15.523 4.477 20 10 20C15.523 20 20 15.523 20 10C20 4.477 15.523 0 10 0ZM15 11H11V15H9V11H5V9H9V5H11V9H15V11Z"
                                                                        fill="#002A7B"></path>
                                                                </svg>
                                                            </span>
                                                        </div>
                                                    </div>
                                                    <div class="row mb-3 mt-2">
                                                        <div class="col-12">
                                                            <h6>Real Estate Experience</h6>
                                                        </div>
                                                        <div class="col-12">
                                                            <label for="DealsDescription" class="form-label text-start">
                                                                What kind of deals have you done in the last 2 years?
                                                            </label>
                                                            <textarea class="form-control"
                                                                placeholder="Enter description.."
                                                                id="DealsDescription"></textarea>
                                                        </div>
                                                        <div class="col-12">

                                                        </div>
                                                    </div>
                                                </div>
                                                <button type="button" name="next"
                                                    class="next btn-sign btn-pbr float-end">Next</button>
                                                <button type="button" name="next"
                                                    class="close btn-sign btn-pbr float-end">Save and close</button>
                                                <button type="button" name="previous"
                                                    class="me-2 btn-pbr btn-cancel float-end previous">Previous</button>
                                            </section>
                                            <section class="fieldset" id="tab3">
                                                <div class="form-card">
                                                    <div class="row">
                                                        <div class="col-12">
                                                            <h2 class="fs-title text-capitalize">
                                                                Personal financial statement</h2>
                                                        </div>
                                                        <div class="col-12">
                                                            <h5>Liquidity</h5>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-3">
                                                        <div
                                                            class="col-xl-12 mb-3 d-flex align-items-center sm-d-block ">
                                                            <label class="me-2" for="CashOnHand">
                                                                How much cash on hand do you have?
                                                            </label>
                                                            <input type="tel" class="input-field w-auto" id="CashOnHand"
                                                                placeholder="Total liquidity" value="$">
                                                        </div>
                                                    </div>
                                                </div>
                                                <button type="button" name="next"
                                                    class="next btn-sign btn-pbr float-end">Next</button>
                                                <button type="button" name="next"
                                                    class="close btn-sign btn-pbr float-end" value="">Save and
                                                    Close</button>
                                                <button type="button" name="previous"
                                                    class="me-2 btn-pbr btn-cancel float-end previous">Previous</button>
                                            </section>
                                            <section class="fieldset" id="tab4">
                                                <div class="form-card mb-3">
                                                    <div class="row">
                                                        <div class="col-12">
                                                            <h2 class="fs-title text-capitalize">
                                                                Personal financial statement</h2>
                                                        </div>
                                                        <div class="col-12">
                                                            <h5>Real Estate Owned</h5>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-3">
                                                        <div class="col-12 mb-3 d-flex align-items-center sm-d-block">
                                                            <span class="ms-1">
                                                                Do you own any other real estate?
                                                            </span>
                                                            <div>
                                                                <span class="ms-3">
                                                                    <input class="form-check-input" type="radio"
                                                                        name="otherrealestate" id="OtherYes" checked>
                                                                    <label class="form-check-label ms-1"
                                                                        for="OtherYes">Yes</label>
                                                                </span>
                                                                <span class="ms-3">
                                                                    <input class="form-check-input" type="radio"
                                                                        name="otherrealestate" id="OtherNo">
                                                                    <label class="form-check-label ms-1"
                                                                        for="OtherNo">No</label>
                                                                </span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!-------- Below row is hidden and will show on click to yes radio button ------>
                                                    <div class="row mt-2 mb-3">
                                                        <div class="col-12">
                                                            <div class="row mb-2">
                                                                <div class="col-xl-6">
                                                                    <div class="row">
                                                                        <div class="col-xl-6 col-md-6 mb-2">
                                                                            <label for="estateaddress1"
                                                                                class="form-label">
                                                                                Address line 1</label>
                                                                            <input type="text" class="input-field"
                                                                                id="estateaddress1"
                                                                                placeholder="Address l">
                                                                        </div>
                                                                        <div class="col-xl-6 col-md-6 mb-2">
                                                                            <label for="estateaddress2"
                                                                                class="form-label">
                                                                                Address line 2</label>
                                                                            <input type="text" class="input-field"
                                                                                id="estateaddress2"
                                                                                placeholder="Address 2">
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-xl-6">
                                                                    <div class="row">
                                                                        <div class="col-xl-4 col-md-6 mb-2">
                                                                            <label for="estateCityField"
                                                                                class="form-label">City</label>
                                                                            <input type="text" class="input-field"
                                                                                id="estateCityField" placeholder="City">
                                                                        </div>
                                                                        <div class="col-xl-4 col-md-6 mb-2">
                                                                            <label for="estatestateField"
                                                                                class="form-label">State</label>
                                                                            <input type="text" class="input-field"
                                                                                id="estatestateField"
                                                                                placeholder="State">
                                                                        </div>
                                                                        <div class="col-xl-4 col-md-6 mb-2">
                                                                            <label for="estatezipCode"
                                                                                class="form-label">Zip
                                                                                code</label>
                                                                            <input type="text" class="input-field"
                                                                                id="estatezipCode"
                                                                                placeholder="Zip code">
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="row Descnote" id="ownState">
                                                                <div class="col-xxl-2 col-xl-3 col-md-6 mb-2">
                                                                    <label for="fixAdjustRate" class="form-label">Type
                                                                        of
                                                                        property</label>
                                                                    <select class="form-select"
                                                                        aria-label="Default select example"
                                                                        id="assetTypeId">
                                                                        <option selected>Select</option>
                                                                        <option value="1">Multifamily</option>
                                                                        <option value="2">1-4 Residential</option>
                                                                        <option value="3">Office</option>
                                                                        <option value="4">Industrial</option>
                                                                        <option value="5">Retail</option>
                                                                        <option value="6">Shopping Center</option>
                                                                        <option value="7">Specialty</option>
                                                                        <option value="8">Healthcare
                                                                        </option>
                                                                        <option value="9">Hospitality</option>
                                                                        <option value="10">Sports &amp; Entertainment
                                                                        </option>
                                                                        <option value="11">Land</option>
                                                                        <option value="12">Residential Portfolio
                                                                        </option>
                                                                    </select>
                                                                </div>
                                                                <div class="col-xxl-2 col-xl-3 col-md-6 mb-2">
                                                                    <label for="PropertyWorth" class="form-label">What
                                                                        is the
                                                                        property worth?</label>
                                                                    <input type="text" class="input-field"
                                                                        id="PropertyWorth" placeholder="Property worth">
                                                                </div>
                                                                <div class="col-xxl-2 col-xl-3 col-md-6 mb-2">
                                                                    <label for="PropertyDebt" class="form-label">
                                                                        How much debt on property?</label>
                                                                    <input type="tel" class="input-field"
                                                                        id="PropertyDebt"
                                                                        placeholder="Debt on property">
                                                                </div>
                                                                <div class="col-xxl-2 col-xl-3 col-md-6 mb-2">
                                                                    <label for="acquiredtime" class="form-label">
                                                                        Month/year acquired</label>
                                                                    <input type="date" class="input-field"
                                                                        id="acquiredtime">
                                                                </div>
                                                                <div class="col-xxl-2 col-xl-3 col-md-6 mb-2">
                                                                    <label for="percentowned" class="form-label">
                                                                        Percentage of property owned</label>
                                                                    <input type="text" class="input-field"
                                                                        id="percentowned"
                                                                        placeholder="Percentage owned">
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-xxl-3 col-xl-3 col-md-4">
                                                            <button type="button" name="next"
                                                                class="close btn-sign btn-pbr">Add
                                                                more
                                                                <span class="ms-2">
                                                                    <svg xmlns="http://www.w3.org/2000/svg" width="20"
                                                                        height="20" viewBox="0 0 20 20" fill="none">
                                                                        <path
                                                                            d="M10 0C4.477 0 0 4.477 0 10C0 15.523 4.477 20 10 20C15.523 20 20 15.523 20 10C20 4.477 15.523 0 10 0ZM15 11H11V15H9V11H5V9H9V5H11V9H15V11Z"
                                                                            fill="#fff"></path>
                                                                    </svg>
                                                                </span></button>
                                                        </div>
                                                    </div>

                                                    <!-------- Above row is hidden and will show on click to yes radio button ------>
                                                </div>
                                                <button type="button" name="next"
                                                    class="next btn-sign btn-pbr float-end">Next</button>
                                                <button type="button" name="next"
                                                    class="close btn-sign btn-pbr float-end">Save and Close</button>
                                                <button type="button" name="previous"
                                                    class="me-2 btn-pbr btn-cancel float-end previous">Previous</button>
                                            </section>
                                            <section class="fieldset" id="tab5">
                                                <div class="form-card mb-3">
                                                    <div class="row">
                                                        <div class="col-12">
                                                            <h2 class="fs-title">Loans And Notes</h2>
                                                        </div>
                                                        <div class="row mb-2">
                                                            <div
                                                                class="col-12 mb-3 d-flex align-items-center sm-d-block">
                                                                <span class="ms-1">
                                                                    Are any loans and notes due <span
                                                                        class="text-underline">to</span> you?
                                                                </span>
                                                                <div>
                                                                    <span class="ms-3">
                                                                        <input class="form-check-input" type="radio"
                                                                            name="loansornotes" id="LoansYes" checked>
                                                                        <label class="form-check-label ms-1"
                                                                            for="LoansYes">Yes</label>
                                                                    </span>
                                                                    <span class="ms-3">
                                                                        <input class="form-check-input" type="radio"
                                                                            name="loansornotes" id="LoansNo">
                                                                        <label class="form-check-label ms-1"
                                                                            for="LoansNo">No</label>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <!-------- Below row is hidden and will show on click to yes radio button ------>
                                                        <div class="row" id="Descnote">
                                                            <div class="col-xl-3 col-md-6 mb-2">
                                                                <label for="NoteDescrptn" class="form-label">
                                                                    Description of note</label>
                                                                <input type="text" class="input-field" id="NoteDescrptn"
                                                                    placeholder="Note description">
                                                            </div>
                                                            <div class="col-xxl-2 col-xl-3 col-md-6 mb-3 w-xl-10">
                                                                <label class="form-label">Collateralized?</label>
                                                                <div class="mt-2 mb-3">
                                                                    <div class="form-check form-check-inline">
                                                                        <input class="form-check-input" type="radio"
                                                                            name="Collateralizedloans"
                                                                            id="collateralizedYes" checked>
                                                                        <label class="form-check-label"
                                                                            for="collateralizedYes">Yes</label>
                                                                    </div>
                                                                    <div class="form-check form-check-inline">
                                                                        <input class="form-check-input" type="radio"
                                                                            name="Collateralizedloans"
                                                                            id="collateralizedYNo">
                                                                        <label class="form-check-label"
                                                                            for="collateralizedYNo">No</label>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-xxl-2 col-xl-3 col-md-6 mb-2">
                                                                <label for="collateral" class="form-label">
                                                                    What collateral?</label>
                                                                <input type="text" class="input-field" id="collateral"
                                                                    placeholder="Collateral">
                                                            </div>
                                                            <div class="col-xxl-2 col-xl-3 col-md-6 mb-2">
                                                                <label for="Notevalue" class="form-label">
                                                                    Note value</label>
                                                                <input type="tel" class="input-field" id="Notevalue"
                                                                    placeholder="Note value">
                                                            </div>
                                                            <div class="col-xxl-2 col-xl-3 col-md-6 mb-2">
                                                                <label for="collateralvalue" class="form-label">
                                                                    Value of collateral</label>
                                                                <input type="tel" class="input-field"
                                                                    id="collateralvalue" placeholder="Collateral value">
                                                            </div>
