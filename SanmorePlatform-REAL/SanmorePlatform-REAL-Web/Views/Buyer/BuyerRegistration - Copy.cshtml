                                                </div>
                                            </div> <br>
                                            <!-- fieldsets -->
                                            <section class="fieldset step" id="tab1">
                                                <div class="form-card">
                                                    <div class="row">
                                                        <div class="col-12">
                                                            <h2 class="fs-title">
                                                                Please Confirm The Information
                                                                <!-- Please confirm the information on file -->
                                                            </h2>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-3">
                                                        <!-- <h5>Personal Information</h5> -->
