                                                            <label for="fName" class="form-label">First Name</label>
                                                            <input type="hidden" id="hdnSectionNameSavedUpto" value="@Model.SectionID">
                                                            <input type="text" class="input-field" id="txtFName"
                                                                   placeholder="First Name" value="@Model.FirstName" disabled>
                                                            <input type="hidden" class="input-field" id="hdnUserId" placeholder="First Name" value="@Model.UserID" disabled>

                                                        </div>
                                                        <div class="col-xl-3 col-md-6 mb-3">
                                                            <label for="lName" class="form-label">Last Name</label>
                                                            <input type="text" class="input-field" id="txtLName"
                                                                   placeholder="Last Name" value="@Model.LastName" disabled>
                                                        </div>
                                                        <div class="col-xl-3 col-md-6 mb-3">
                                                            <label for="mobilePhoneId" class="form-label">Phone</label>
                                                            <input type="tel" class="input-field" id="txtMobilePhoneId"
                                                                   value="@Model.PhoneNumber" disabled>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-2 mb-3">
                                                        <div class="col-xl-6">
                                                            <div class="row">
                                                                <div class="col-xl-6 col-md-6 mb-2" id="formGroupAddress1">
                                                                    <label for="address1" class="form-label">
                                                                        Address line 1<span class="red-txt">*</span>
                                                                    </label>
                                                                    <input type="text" class="input-field" id="txtAddress1" value="@Model.AddressLine1" placeholder="Address l">
                                                                    <span class="text-danger" id="errAddress1"></span>
                                                                </div>
                                                                <div class="col-xl-6 col-md-6 mb-2">
                                                                    <label for="address2" class="form-label">
                                                                        Address line 2
