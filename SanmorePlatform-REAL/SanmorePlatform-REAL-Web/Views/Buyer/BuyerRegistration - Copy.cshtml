@model SanmorePlatform_REAL_Model.ViewModels.BuyerModels.BuyerGetViewModel
@{
    Layout = "_Layout";
}
<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Fast Track Buyer : SanmorePlatform-REAL</title>
    <!-- Links -->
    <!-- Lenders CSS -->
    <link rel="stylesheet" href="~/css/lenders.css">
    <link href="~/css/fasttrackbuyer.css" rel="stylesheet" />

</head>

<body>
    @* End Real Estate Owned *@
    @* hidden div for clone Loan And Notes *@
    <div id="LoanandNoteDivhdn" class="d-none">
        <div class="row line-bottom-dasshed">
            <h5 class="mb-2 mt-2" id="bingLoanAndNoteCounts">Loans And Notes Details 1</h5>
            <div class="row">
                <div class="col-xxl-6 col-xl-6 col-12">
                    <div class="">
                        <h6 class="h6 fw-bold mb-0">
                            Description of note
                        </h6>
                        <p id="bindLoanDesc">

                        </p>
                    </div>
                </div>
                <div class="col-xxl-3 col-xl-3 col-md-4 col-12">
                    <div class="">
                        <h6 class="h6 fw-bold mb-0">
                            Collateralized?
                        </h6>
                        <p id="bindCollateral"></p>
                    </div>
                </div>
                <div class="col-xxl-3 col-xl-3 col-md-4 col-12">
                    <div class="">
                        <h6 class="h6 fw-bold mb-0">
                            What collateral?
                        </h6>
                        <p id="bindWhatCollateral"></p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xxl-4 col-xl-4 col-md-4 col-12">
                    <div class="">
                        <h6 class="h6 fw-bold mb-0">
                            Note value
                        </h6>
                        <p id="bindNoteValue"></p>
                    </div>
                </div>
                <div class="col-xxl-3 col-xl-3 col-md-4 col-6 col-12">
                    <div class="">
                        <h6 class="h6 fw-bold mb-0">
                            Value of collateral
                        </h6>
                        <p id="bindCollateralValue"></p>
                    </div>
                </div>
            </div>
        </div>
    </div>

    @* End Loan And Notes*@
    <div id="BuisnessOwnedDivhdn" class="d-none">
        <div class="row line-bottom-dasshed">
            <h5 class="mb-2 mt-2" id="bindBuisnesscount">Business</h5>
            <div class="col-xxl-3 col-xl-4 col-md-3 col-12">
                <div class="">
                    <h6 class="h6 fw-bold mb-0">
                        Type of business
                    </h6>
                    <p id="bindBuisnessType"></p>
                </div>
            </div>
            <div class="col-xxl-3 col-xl-4 col-md-6 col-12">
                <div class="">
                    <h6 class="h6 fw-bold mb-0">
                        Percentage of business owned
                    </h6>
                    <p id="bindBuisnessPercentage"></p>
                </div>
            </div>
            <div class="col-xxl-3 col-xl-4 col-md-3 col-12">
                <div class="">
                    <h6 class="h6 fw-bold mb-0">
                        Value of business
                    </h6>
                    <p id="bindBuisnessValue"></p>
                </div>
            </div>
        </div>
    </div>

    <div id="OtherAssetsOwnedDivhdn" class="d-none">
        <div class="row line-bottom-dasshed">
            <h5 class="mb-2 mt-2" id="bindOtherAssetsDetailsCount"></h5>
            <div class="col-xxl-4 col-xl-6 col-12">
                <div class="">
                    <h6 class="h6 fw-bold mb-0">
                        Other assets total worth(Jewelry,
                        Furniture, Art and others)
                    </h6>
                    <p id="bindOtherAssetsTotalWorth"></p>
                </div>
            </div>
            <div class="col-xxl-4 col-xl-6 col-12">
                <div class="">
                    <h6 class="h6 fw-bold mb-0">
                        Debt on assets owned(Jewelry,
                        Furniture, Art and others)
                    </h6>
                    <p id="bindDebtsOnAssets"></p>
                </div>
            </div>
        </div>
    </div>
    <div id="OtherDebtsDivhdn" class="d-none">
        <div class="row line-bottom-dasshed">
            <h5 class="mb-2 mt-2" id="bindOtherDebtsDetailsCount"></h5>
            <div class="col-xxl-4 col-md-5">
                <div class="">
                    <h6 class="h6 fw-bold mb-0">
                        Do you have any other debts?
                    </h6>
                    <p id="bindHaveYouAnyOtherDebts"></p>
                </div>
            </div>
            <div class="col-xxl-4 col-xl-6 col-md-4 col-12">
                <div class="">
                    <h6 class="h6 fw-bold mb-0">
                        Type of Debt
                    </h6>
                    <p id="bindTypeOfDebts"></p>
                </div>
            </div>
            <div class="col-xxl-4 col-xl-6 col-md-3 col-12">
                <div class="">
                    <h6 class="h6 fw-bold mb-0">
                        Debt amount
                    </h6>
                    <p id="bindDebtAmount"></p>
                </div>
            </div>
        </div>
    </div>

    <!-- top section -->
    <div class="top-section">
        <div class="container-fluid container-px">
            <h3 class="fw-semibold mb-0">
                <span class="blue-primary">Fast Track Buyer</span> - Application
            </h3>
        </div>
    </div>
    <div class="addlistingform min-50vh">
        <div class="container-fluid container-px">
            <div class="add-property">
