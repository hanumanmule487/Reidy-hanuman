                                                                        <input type="text" class="input-field" value="@Model.ZipCode" id="txtZipCode" placeholder="Zip code" maxlength="5" minlength="5">
                                                                    }
                                                                    else
                                                                    {
                                                                        <input type="text" class="input-field" id="txtZipCode" placeholder="Zip code" maxlength="5" minlength="5">
                                                                    }
                                                                    <span class="text-danger" id="errZipCode"></span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </section>




